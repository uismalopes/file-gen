{
  "name": "file-gen",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "file-gen": "./dist/index.js"
  },
  "scripts": {
    "start": "nodemon --",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "test": "jest",
    "create": "npm run build && npm run test",
    "build": "rimraf ./lib && tsc -p . && tsc-alias",
    "local": "npm i -g && file-gen",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^13.1.0",
    "figlet": "^1.8.0",
    "inquirer": "^12.5.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}